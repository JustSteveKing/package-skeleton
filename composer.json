{
  "name": "juststeveking/package-name",
  "description": "Package Description",
  "license": "MIT",
  "authors": [
    {
      "role": "Developer",
      "name": "Steve McDougall",
      "email": "juststevemcd@gmail.com",
      "homepage": "https://www.juststeveking.uk/"
    }
  ],
  "autoload": {
    "psr-4": {
      "JustSteveKing\\Namespace\\": "src/"
    }
  },
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "laravel/pint": "^1.9",
    "pestphp/pest": "^2.5.2",
    "phpstan/phpstan": "^1.10.14"
  },
  "scripts": {
    "pint": [
      "./vendor/bin/pint"
    ],
    "stan": [
      "./vendor/bin/phpstan analyse"
    ],
    "test": [
      "./vendor/bin/pest"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.4"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
